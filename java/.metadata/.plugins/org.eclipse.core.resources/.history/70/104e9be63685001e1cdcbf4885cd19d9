package com.bstn.zplviewer.graphics;

import java.io.File;

import com.bstn.zplviewer.renderables.Code128Barcode;
import com.bstn.zplviewer.renderables.Graphic;
import com.bstn.zplviewer.renderables.Rectangle;
import com.bstn.zplviewer.renderables.Text;

public interface Renderer {
	
	abstract void drawText(Text text);
	abstract void drawRectangle(Rectangle rectangle);
	abstract void drawCode128Barcode(Code128Barcode code128Barcode);
	abstract void drawGraphic(Graphic graphic);
	abstract boolean save(File file);

}
